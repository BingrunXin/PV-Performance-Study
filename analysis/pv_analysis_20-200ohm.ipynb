    "# Cell 10 — Global I–V and P–V curves assembled from block means (one point per block)\n",
    "# We will order blocks by Resistance_Ohm (numeric) if available, else by block order.\n",
    "df_pts = blocks_summary_df.copy()\n",
    "# drop empty-block rows with n_rows == 0\n",
    "df_pts = df_pts[df_pts['n_rows'] > 0].copy()\n",
    "\n",
    "# Determine plotting order\n",
    "if df_pts['Resistance_Ohm'].notna().any():\n",
    "    # order by resistance numeric\n",
    "    df_pts = df_pts.sort_values('Resistance_Ohm')\n",
    "else:\n",
    "    df_pts = df_pts.sort_index()\n",
    "\n",
    "# I–V: Voltage vs Current (points = block means)\n",
    "fig_iv, ax_iv = plt.subplots(figsize=(7,5))\n",
    "ax_iv.scatter(df_pts['mean_voltage_V'], df_pts['mean_current_A'], s=60)\n",
    "ax_iv.set_xlabel('Mean Voltage (V) per block')\n",
    "ax_iv.set_ylabel('Mean Current (A) per block')\n",
    "ax_iv.set_title('Assembled I–V (one point per resistor block)')\n",
    "f_iv_all = os.path.join(OUT_DIR, 'assembled_IV_points.png')\n",
    "fig_iv.savefig(f_iv_all, dpi=300, bbox_inches='tight')\n",
    "plt.close(fig_iv)\n",
    "print(\"Saved:\", f_iv_all)\n",
    "\n",
    "# P–V: Voltage vs Power (points = block means) + find global MPP among blocks\n",
    "fig_pv_all, ax_pv_all = plt.subplots(figsize=(7,5))\n",
    "ax_pv_all.scatter(df_pts['mean_voltage_V'], df_pts['mean_power_W'], s=60)\n",
    "ax_pv_all.set_xlabel('Mean Voltage (V) per block')\n",
    "ax_pv_all.set_ylabel('Mean Power (W) per block')\n",
    "ax_pv_all.set_title('Assembled P–V (one point per resistor block)')\n",
    "\n",
    "# find block with maximum mean_power_W\n",
    "if df_pts['mean_power_W'].notna().any():\n",
    "    idx_max = df_pts['mean_power_W'].idxmax()\n",
    "    max_row = df_pts.loc[idx_max]\n",
