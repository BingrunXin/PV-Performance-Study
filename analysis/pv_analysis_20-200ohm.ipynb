   "cell_type": "code",
   "source": [
    "# Cell 7 — Build a per-block DataFrame list (parse resistance, preserve original rows)\n",
    "block_dfs = []\n",
    "for i, idx_list in enumerate(blocks, start=1):\n",
    "    block_df = df_raw.loc[idx_list].copy()\n",
    "    block_df.index = range(len(block_df))  # reindex locally for convenience\n",
    "    # parse resistance in this block (the column might be blank except first row)\n",
    "    block_df['_res_parsed'] = block_df[res_col].apply(parse_resistance_value)\n",
    "    # determine the block resistance: prefer first non-null parsed value, else mode\n",
    "    block_res = np.nan\n",
    "    nonnull = block_df['_res_parsed'].dropna()\n",
    "    if not nonnull.empty:\n",
    "        block_res = float(nonnull.iloc[0])  # usually the merged header on first row\n",
    "    else:\n",
    "        # fallback: check raw text for any numeric\n",
    "        for v in block_df[res_col].astype(str):\n",
    "            r = parse_resistance_value(v)\n",
    "            if not np.isnan(r):\n",
    "                block_res = float(r)\n",
    "                break\n",
    "    block_df['Resistance_Ohm'] = block_res\n",
    "    block_dfs.append((i, block_res, block_df))\n",
    "print(f\"Built {len(block_dfs)} block DataFrames.\")\n",
    "# preview the first block's resistance and first rows\n",
    "if block_dfs:\n",
    "    idx0, r0, df0 = block_dfs[0]\n",
    "    print(\"Example block 1 resistance:\", r0)\n",
