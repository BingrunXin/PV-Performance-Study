   "cell_type": "code",
   "source": [
    "# Cell 6 — Split into contiguous non-blank blocks (between blank rows)\n",
    "blocks = []\n",
    "current_idx = []\n",
    "for idx in df_raw.index:\n",
    "    if blank_mask.loc[idx]:\n",
    "        # this is a separator row\n",
    "        if current_idx:\n",
    "            blocks.append(current_idx)\n",
    "            current_idx = []\n",
    "    else:\n",
    "        current_idx.append(idx)\n",
    "# Append last block if any\n",
    "if current_idx:\n",
    "    blocks.append(current_idx)\n",
    "\n",
    "print(\"Detected blocks (groups between blank lines):\", len(blocks))\n",
    "# show first few block sizes\n",
    "for i, b in enumerate(blocks[:8]):\n",
    "    print(f\"Block {i+1}: {len(b)} rows; indices {b[0]} .. {b[-1]}\")\n"
