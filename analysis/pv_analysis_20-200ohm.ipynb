   "cell_type": "code",
   "source": [
    "# Cell 8 — Clean each block: convert numeric columns, remove known DS18B20 error rows, compute Panel_Temp_Avg_C\n",
    "processed_blocks = []\n",
    "for (blk_id, blk_res, bdf) in block_dfs:\n",
    "    # convert all columns to numeric where possible (except original textual columns)\n",
    "    for c in bdf.columns:\n",
    "        # keep the original res_col textual column and _res_parsed; others try numeric\n",
    "        if c in [res_col, '_res_parsed', 'Resistance_Ohm']:\n",
    "            continue\n",
    "        bdf[c] = pd.to_numeric(bdf[c], errors='coerce')\n",
    "    # remove DS18B20 bad rows (value equals DS18B20_ERROR in any panel temp column)\n",
    "    panel_temp_cols = [c for c in bdf.columns if 'panel' in c.lower() and 'temp' in c.lower()]\n",
    "    if panel_temp_cols:\n",
    "        mask_bad = False\n",
    "        for c in panel_temp_cols:\n",
    "            mask_bad = mask_bad | (bdf[c] == DS18B20_ERROR)\n",
    "        if mask_bad.any():\n",
    "            # drop rows with bad sensor reads\n",
    "            bdf = bdf.loc[~mask_bad].copy()\n",
    "    # Panel_Temp_Avg_C: average existing panel temp columns if any\n",
    "    if panel_temp_cols:\n",
    "        available = [c for c in panel_temp_cols if c in bdf.columns]\n",
    "        if available:\n",
    "            bdf['Panel_Temp_Avg_C'] = bdf[available].mean(axis=1, skipna=True)\n",
    "    # Try to detect Light column(s)\n",
    "    light_cols = [c for c in bdf.columns if 'lux' in c.lower() or 'light' in c.lower()]\n",
    "    if light_cols:\n",
    "        bdf['Light_lux'] = bdf[light_cols[0]]\n",
    "    # Standardize Voltage/Current/Power if available in block\n",
    "    vol_cols = [c for c in bdf.columns if 'volt' in c.lower()]\n",
    "    cur_cols = [c for c in bdf.columns if 'current' in c.lower()]\n",
    "    power_cols = [c for c in bdf.columns if 'power' in c.lower()]\n",
    "    if vol_cols:\n",
    "        bdf['Voltage_V'] = bdf[vol_cols[0]]\n",
    "    if cur_cols:\n",
    "        # convert mA->A if column name contains 'ma'\n",
    "        curc = cur_cols[0]\n",
    "        if 'ma' in curc.lower():\n",
    "            bdf['Current_A'] = bdf[curc] / 1000.0\n",
    "        else:\n",
    "            bdf['Current_A'] = bdf[curc]\n",
    "    else:\n",
    "        # if no current column, we can compute I = V / R if V exists and Resistance_Ohm known\n",
    "        if 'Voltage_V' in bdf.columns and not np.isnan(blk_res):\n",
    "            bdf['Current_A'] = bdf['Voltage_V'] / blk_res\n",
    "    if power_cols:\n",
    "        pc = power_cols[0]\n",
    "        if 'mw' in pc.lower():\n",
    "            bdf['Power_W'] = bdf[pc] / 1000.0\n",
    "        else:\n",
    "            bdf['Power_W'] = bdf[pc]\n",
    "    else:\n",
    "        # compute if V and I available\n",
    "        if 'Voltage_V' in bdf.columns and 'Current_A' in bdf.columns:\n",
    "            bdf['Power_W'] = bdf['Voltage_V'] * bdf['Current_A']\n",
    "    # attach metadata\n",
    "    bdf['Block_ID'] = blk_id\n",
    "    bdf['Resistance_Ohm'] = blk_res\n",
    "    processed_blocks.append((blk_id, blk_res, bdf))\n",
    "print(\"Processed blocks:\", len(processed_blocks))\n",
    "# preview block summary for the first block\n",
    "if processed_blocks:\n",
    "    bid, r, bb = processed_blocks[0]\n",
    "    print(\"Block\", bid, \"resistance\", r, \"rows after cleaning:\", bb.shape[0])\n",
    "    display(bb.head())\n"
   ],
