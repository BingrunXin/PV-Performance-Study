    "# Cell 11 (corrected) — Per-block detailed plots (time series, P-V, temp vs power, light vs power)\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import os\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "for blk_id, blk_res, bdf in processed_blocks:\n",
    "    # build a safe, human-readable resistance string for filenames\n",
    "    if pd.isna(blk_res):\n",
    "        res_str = 'nan'\n",
    "    else:\n",
    "        # prefer integer representation when appropriate\n",
    "        try:\n",
    "            if float(blk_res).is_integer():\n",
    "                res_str = f\"{int(float(blk_res))}\"\n",
    "            else:\n",
    "                res_str = f\"{float(blk_res):.2f}\"\n",
    "        except Exception:\n",
    "            # fallback to plain string\n",
    "            res_str = str(blk_res)\n",
    "\n",
    "    out_prefix = os.path.join(OUT_DIR, f\"block_{blk_id:03d}_res_{res_str}\")\n",
    "\n",
    "    # if block has no rows after cleaning, create a small note image and continue\n",
    "    if bdf.shape[0] == 0:\n",
    "        fig, ax = plt.subplots(figsize=(6,3))\n",
    "        ax.axis('off')\n",
    "        ax.text(0.5, 0.5, f\"Block {blk_id}: no valid rows after cleaning\", ha='center', va='center')\n",
    "        fig.savefig(out_prefix + '_note.png', dpi=200, bbox_inches='tight')\n",
    "        plt.close(fig)\n",
    "        continue\n",
    "\n",
    "    # Determine x axis: elapsed minutes if index is datetime-like, otherwise sample index\n",
    "    if isinstance(bdf.index, pd.DatetimeIndex) or pd.api.types.is_datetime64_any_dtype(bdf.index):\n",
    "        # compute elapsed minutes (vectorized, safe)\n",
    "        elapsed = pd.to_datetime(bdf.index).to_series() - pd.to_datetime(bdf.index[0])\n",
    "        elapsed_min = elapsed.dt.total_seconds() / 60.0\n",
    "        x = elapsed_min.values\n",
    "        xlabel = 'Elapsed time (minutes)'\n",
    "    else:\n",
    "        x = np.arange(len(bdf))\n",
    "        xlabel = 'Sample index'\n",
    "\n",
    "    # === Time series: Power (left) and Voltage (right) ===\n",
    "    fig, ax1 = plt.subplots(figsize=(9,4))\n",
    "    if 'Power_W' in bdf.columns:\n",
    "        ax1.plot(x, bdf['Power_W'], marker='o', ms=4, linewidth=1, label='Power (W)')\n",
    "    ax1.set_xlabel(xlabel)\n",
    "    ax1.set_ylabel('Power (W)')\n",
    "    ax1.set_title(f'Block {blk_id} — Time series (res {res_str} Ω)')\n",
    "    ax2 = ax1.twinx()\n",
    "    if 'Voltage_V' in bdf.columns:\n",
    "        ax2.plot(x, bdf['Voltage_V'], linestyle='--', marker='x', ms=4, label='Voltage (V)')\n",
    "        ax2.set_ylabel('Voltage (V)')\n",
    "    # combined legend\n",
    "    h1, l1 = ax1.get_legend_handles_labels()\n",
    "    h2, l2 = ax2.get_legend_handles_labels()\n",
    "    if h1 or h2:\n",
    "        ax1.legend(h1 + h2, l1 + l2, loc='upper left')\n",
    "    fig.savefig(out_prefix + '_timeseries.png', dpi=300, bbox_inches='tight')\n",
    "    plt.close(fig)\n",
    "\n",
    "    # === P-V scatter within block ===\n",
    "    if 'Voltage_V' in bdf.columns and 'Power_W' in bdf.columns:\n",
    "        fig, ax = plt.subplots(figsize=(6,5))\n",
    "        ax.scatter(bdf['Voltage_V'], bdf['Power_W'], s=16)\n",
    "        ax.set_xlabel('Voltage (V)')\n",
    "        ax.set_ylabel('Power (W)')\n",
    "        ax.set_title(f'Block {blk_id} — P–V (res {res_str} Ω)')\n",
    "        fig.savefig(out_prefix + '_PV_scatter.png', dpi=300, bbox_inches='tight')\n",
    "        plt.close(fig)\n",
    "\n",
    "    # === Panel Temp avg vs Power (Pearson r only) ===\n",
    "    if 'Panel_Temp_Avg_C' in bdf.columns and 'Power_W' in bdf.columns:\n",
