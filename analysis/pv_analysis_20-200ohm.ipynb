   "source": [
    "# Cell 5 — Find the resistance column name (robust) and mark blank rows\n",
    "res_col = None\n",
    "for c in df_raw.columns:\n",
    "    if c in RES_COL_CANDIDATES or c.lower() in [x.lower() for x in RES_COL_CANDIDATES]:\n",
    "        res_col = c\n",
    "        break\n",
    "if res_col is None:\n",
    "    for c in df_raw.columns:\n",
    "        if 'res' in c.lower() or 'load' in c.lower():\n",
    "            res_col = c\n",
    "            break\n",
    "if res_col is None:\n",
    "    raise RuntimeError(\"Resistance column could not be detected automatically. Rename it to one of: \" + str(RES_COL_CANDIDATES))\n",
    "\n",
    "# Mark blank (separator) rows\n",
    "blank_mask = df_raw.apply(lambda r: is_blank_row_series(r), axis=1)\n",
    "print(f\"Total rows: {len(df_raw)}; blank (separator) rows: {blank_mask.sum()}\")\n"
   ],
