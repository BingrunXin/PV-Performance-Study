    "    Pmax = max_row['mean_power_W']\n",
    "    R_opt = max_row['Resistance_Ohm']\n",
    "    ax_pv_all.scatter([Vmp], [Pmax], s=220, marker='*', edgecolors='k', linewidths=0.8)\n",
    "    ann = f\"Pmax = {Pmax:.4f} W\\nVmp = {Vmp:.3f} V\\nR_opt = {R_opt} Ω\"\n",
    "    ax_pv_all.annotate(ann, xy=(Vmp, Pmax), xytext=(0.02,0.95), textcoords='axes fraction',\n",
    "                       verticalalignment='top', bbox=dict(facecolor='white', alpha=0.8))\n",
    "f_pv_all = os.path.join(OUT_DIR, 'assembled_PV_points_with_MPP.png')\n",
    "fig_pv_all.savefig(f_pv_all, dpi=300, bbox_inches='tight')\n",
