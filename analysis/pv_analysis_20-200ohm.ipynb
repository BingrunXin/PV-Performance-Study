   "source": [
    "# Cell 9 — Build a block-summary table (one row per block / resistance)\n",
    "summary_rows = []\n",
    "for (blk_id, blk_res, bdf) in processed_blocks:\n",
    "    if bdf.shape[0] == 0:\n",
    "        # empty after cleaning\n",
    "        summary_rows.append({\n",
    "            'Block_ID': blk_id,\n",
    "            'Resistance_Ohm': blk_res,\n",
    "            'n_rows': 0,\n",
    "            'mean_voltage_V': np.nan,\n",
    "            'mean_current_A': np.nan,\n",
    "            'mean_power_W': np.nan,\n",
    "            'std_power_W': np.nan,\n",
    "            'mean_light_lux': np.nan,\n",
    "            'mean_panel_temp_C': np.nan\n",
    "        })\n",
    "        continue\n",
    "    mv = float(bdf['Voltage_V'].mean()) if 'Voltage_V' in bdf.columns else np.nan\n",
    "    mi = float(bdf['Current_A'].mean()) if 'Current_A' in bdf.columns else np.nan\n",
    "    mp = float(bdf['Power_W'].mean()) if 'Power_W' in bdf.columns else np.nan\n",
    "    sp = float(bdf['Power_W'].std()) if 'Power_W' in bdf.columns else np.nan\n",
    "    ml = float(bdf['Light_lux'].mean()) if 'Light_lux' in bdf.columns else np.nan\n",
    "    mt = float(bdf['Panel_Temp_Avg_C'].mean()) if 'Panel_Temp_Avg_C' in bdf.columns else np.nan\n",
    "    summary_rows.append({\n",
    "        'Block_ID': blk_id,\n",
    "        'Resistance_Ohm': blk_res,\n",
    "        'n_rows': int(bdf.shape[0]),\n",
    "        'mean_voltage_V': mv,\n",
    "        'mean_current_A': mi,\n",
    "        'mean_power_W': mp,\n",
    "        'std_power_W': sp,\n",
    "        'mean_light_lux': ml,\n",
    "        'mean_panel_temp_C': mt\n",
    "    })\n",
    "\n",
    "blocks_summary_df = pd.DataFrame(summary_rows).set_index('Block_ID').sort_index()\n",
    "summary_csv = os.path.join(OUT_DIR, 'blocks_summary.csv')\n",
    "blocks_summary_df.to_csv(summary_csv)\n",
    "print(\"Saved block summary to:\", summary_csv)\n",
    "display(blocks_summary_df.head(40))\n"
